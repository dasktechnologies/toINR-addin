<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
  <Id>acaeb2fc-c617-40f6-8f00-d968afab55b4</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>DASK Technologies</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="toINR"/>
  <Description DefaultValue="Easily convert numeric values into Indian Rupee currency words within Excel."/>
  <IconUrl DefaultValue="https://dasktechnologies.github.io/toINR-addin/assets/icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://dasktechnologies.github.io/toINR-addin/assets/icon-64.png"/>
  <SupportUrl DefaultValue="https://github.com/dasktechnologies/toINR-addin/issues"/>
  <AppDomains>
    <AppDomain>https://dasktechnologies.github.io</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Workbook"/>
  </Hosts>
  <Requirements>
    <Sets DefaultMinVersion="1.1">
      <Set Name="SharedRuntime" MinVersion="1.1"/>
    </Sets>
  </Requirements>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://dasktechnologies.github.io/toINR-addin/taskpane.html"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Workbook">
        <Runtimes>
          <Runtime resid="Taskpane.Url" lifetime="long"/>
        </Runtimes>
        <AllFormFactors>
          <ExtensionPoint xsi:type="CustomFunctions">
            <Script>
              <SourceLocation resid="Functions.Script.Url"/>
            </Script>
            <Page>
              <SourceLocation resid="Taskpane.Url"/>
            </Page>
            <Metadata>
              <SourceLocation resid="Functions.Metadata.Url"/>
            </Metadata>
            <Namespace resid="Functions.Namespace"/>
          </ExtensionPoint>
        </AllFormFactors>
        <DesktopFormFactor>
          <GetStarted>
            <Title resid="GetStarted.Title"/>
            <Description resid="GetStarted.Description"/>
            <LearnMoreUrl resid="GetStarted.LearnMoreUrl"/>
          </GetStarted>
          <FunctionFile resid="Taskpane.Url"/>
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <OfficeTab id="TabHome">
              <Group id="CommandsGroup">
                <bt:Label resid="CommandsGroup.Label"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>
                <Control xsi:type="Button" id="TaskpaneButton">
                  <bt:Label resid="TaskpaneButton.Label"/>
                  <bt:Supertip>
                    <bt:Title resid="TaskpaneButton.Label"/>
                    <bt:Description resid="TaskpaneButton.Tooltip"/>
                  </bt:Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>ButtonId1</TaskpaneId>
                    <SourceLocation resid="Taskpane.Url"/>
                  </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <bt:Image id="Icon.16x16" DefaultValue="https://dasktechnologies.github.io/toINR-addin/assets/toINR_16.png"/>
        <bt:Image id="Icon.32x32" DefaultValue="https://dasktechnologies.github.io/toINR-addin/assets/toINR_32.png"/>
        <bt:Image id="Icon.80x80" DefaultValue="https://dasktechnologies.github.io/toINR-addin/assets/toINR_80.png"/>
      </bt:Images>
      <bt:Urls>
        <bt:Url id="Functions.Script.Url" DefaultValue="https://dasktechnologies.github.io/toINR-addin/functions.js"/>
        <bt:Url id="Functions.Metadata.Url" DefaultValue="https://dasktechnologies.github.io/toINR-addin/functions.json"/>
        <bt:Url id="GetStarted.LearnMoreUrl" DefaultValue="https://github.com/dasktechnologies/toINR-addin"/>
        <bt:Url id="Taskpane.Url" DefaultValue="https://dasktechnologies.github.io/toINR-addin/taskpane.html"/>
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="Functions.Namespace" DefaultValue="toINR"/>
        <bt:String id="GetStarted.Title" DefaultValue="toINR: Number to Rupee Converter"/>
        <bt:String id="CommandsGroup.Label" DefaultValue="toINR Tools"/>
        <bt:String id="TaskpaneButton.Label" DefaultValue="Help &amp; Instructions"/>
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="GetStarted.Description" DefaultValue="Successfully loaded! Use =toINR.convertToRupees(cell) to convert numbers to words."/>
        <bt:String id="TaskpaneButton.Tooltip" DefaultValue="Click for help with Rupee conversions"/>
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>
